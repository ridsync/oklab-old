<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
  </head>
  <body>
    <ul id="account">

    </ul>
    <h1><a href="/auth/login">OK Nodejs Login Page</a></h1>

    <form action="/auth/login" method="post">
      <p>
        <input type="text" name="username" placeholder="username">
      </p>
      <p>
        <input type="password" name="password" placeholder="password">
      </p>
      <p>
        <input type="submit">
      </p>
    </form><a href="/auth/facebook">facebook</a>

    <ul id="accountslist">
      <% users.forEach(function(user) { %>
        <li><a href="/auth/register">user = <%= user.username %></a></li>
      <% }); %>

    </ul>
  </body>
</html>
